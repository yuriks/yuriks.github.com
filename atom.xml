---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>yuriks.net</title>
	<link href="http://yuriks.net/atom.xml" rel="self"/>
	<link href="http://yuriks.net/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://yuriks.net/</id>
	<author>
		<name>Yuri Kunde Schlesner</name>
		<email>yuriks@yuriks.net</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://yuriks.net{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://yuriks.net{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
